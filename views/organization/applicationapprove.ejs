<!DOCTYPE html>
<html>
<head>
	<title>Update profile</title>
</head>
<body>
	<h1>Profile Update</h1>
	<a href="/organization/application">Back</a> | 
	<a href="/logout">logout</a>
	
	<form method="post">
	<table border="1">
		<tr>
			<td> ID</td>
			<td><input type="text" name="id" value="<%= users[0].ID %>"></td>
		</tr>
		<tr>
			<td>Name</td>
			<td><input type="text" name="name" value="<%= users[0].name %>"></td>
		</tr>
		<tr>
			<td>University Name</td>
			<td><input type="text" name="university" value="<%= users[0].university %>"></td>
		</tr>
		<tr>
			<td>Email</td>
			<td><input type="text" name="email" value="<%= users[0].email %>"></td>
		</tr>
		<tr>
			<td>Phone Number</td>
			<td><input type="text" name="contract" value="<%= users[0].contract %>"></td>
		</tr>
		<tr>
			<td>Program</td>
			<td><input type="text" name="program" value="<%= users[0].program %>"></td>
		</tr>
		<tr>
			<td>Cgpa</td>
			<td><input type="text" name="cgpa" value="<%=users[0].cgpa %>"></td>
		</tr>
		<tr>
			<td colspan="2">
				<center>
						<input type="submit" name="submit" value="Confirm"></td>
				</center>
			
		</tr>
	</table>
</form>

</body>
</html>