<!DOCTYPE html>
<html>
<head>
<style>
ul {
  list-style-type: none;
  margin: 0;
  padding: 0;
  overflow: hidden;
  background-color: #105A43;
}

li {
  float: left;
  width: 25%;
}

li a {
  display: block;
  color: white;
  text-align: center;
  padding: 14px 25px;
  text-decoration: none;
}

li a:hover {
  background-color: #06261D;
}
</style>
</head>
<body>
	<table border="0" width="100%">
		
		<tr class="heading">
			<td>
				<p><font style="font-style: italic;" color="#313D3A" size="8"><b>Scholarship Portal</b></font></p>
			</td>
		</tr>
		<tr>
			<td>
				<ul>
					<li><a href="/admin">Home</a></li>
					<li><a href="/admin/search">Search Admin</a></li>
					<li><a href="/admin/addadmin">Add Admin</a></li>
					<li><a href="/logout">Log Out</a></li>
				</ul>
			</td>
		</tr>
		<% for(var i=0; i < userdetails.length; i++){ %>
		<tr>
			<td height="100px" align="center" valign="middle">

				<font size="5">
					<a href="/admin/modifyadmin/<%= userdetails[i].userid %>"><%= userdetails[i].username %></a><br><br>
				</font>			
			</td>
		</tr>
		<% } %>
	
	</table>





</body>
</html>